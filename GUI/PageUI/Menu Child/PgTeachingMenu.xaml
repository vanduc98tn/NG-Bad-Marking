<Page x:Class="GUI.PgTeachingMenu"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:PLC="clr-namespace:DevicePLC;assembly=DevicePLC"
      xmlns:local="clr-namespace:GUI"
      mc:Ignorable="d" 
      d:DesignHeight="1024" d:DesignWidth="1280"
      Title="PgTeachingMenu">

    <Grid Background="#dddddd">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="6*"></ColumnDefinition>
            <ColumnDefinition Width="1*"></ColumnDefinition>
        </Grid.ColumnDefinitions>

        <Grid Grid.Column="0" Margin="5,0,2,2" Name="gridLeft">
            <Grid.RowDefinitions>
                <RowDefinition Height="0.7*"></RowDefinition>
                <RowDefinition Height="2*"></RowDefinition>
                <RowDefinition Height="4*"></RowDefinition>
                <RowDefinition Height="4*"></RowDefinition>
            </Grid.RowDefinitions>
            <Border Grid.Row="0" Grid.ColumnSpan="2" BorderThickness="1" BorderBrush="#bbbbbb" MinHeight="60">
                <Grid Grid.Row="0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="5*" />
                        <ColumnDefinition Width="1*" />
                    </Grid.ColumnDefinitions>
                    <Grid Grid.Column="0" Background="DarkOrange">
                        <Label Grid.Column="0" Foreground="#EEEEEE" FontSize="20" FontWeight="Bold"
                           Margin="20,0,0,0" VerticalContentAlignment="Top">
                            Base Pos. Manual Teaching: [Robot]
                        </Label>
                    </Grid>

                    <TextBlock Grid.Column="1" x:Name="tvLog" FontSize="20"
                           HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
                    <Button Grid.Column="1" x:Name="btSave" Margin="110,0,0,0" HorizontalContentAlignment="Center" BorderThickness="1" BorderBrush="Transparent" Background="Transparent" VerticalContentAlignment="Center">
                        <Image Source="/Resource/Image/menu-save.png"></Image>
                    </Button>

                </Grid>

                <!--<TextBlock Foreground="Orange" FontSize="24" Margin="20,0,0,0" VerticalAlignment="Center">
                Base Teaching
            </TextBlock>-->
            </Border>
            <Grid Grid.Row="1">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="2*"></ColumnDefinition>

                    <ColumnDefinition Width="0.5*"></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <Grid Grid.Column="0">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="1.2*"></RowDefinition>
                        <RowDefinition Height="5*"></RowDefinition>
                    </Grid.RowDefinitions>
                    <Label x:Name="lblNotification" Grid.Row="0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="13" FontWeight="Bold"  ></Label>
                    <Grid Grid.Row="1">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="1*"></RowDefinition>
                            <RowDefinition Height="1*"></RowDefinition>
                            <RowDefinition Height="1*"></RowDefinition>
                            <RowDefinition Height="1*"></RowDefinition>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="1*"></ColumnDefinition>
                            <ColumnDefinition Width="1*"></ColumnDefinition>
                            <ColumnDefinition Width="1*"></ColumnDefinition>
                            <ColumnDefinition Width="1*"></ColumnDefinition>
                            <ColumnDefinition Width="1*"></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <Label VerticalContentAlignment="Center" Grid.Row="0" Grid.Column="1" FontSize="12" HorizontalContentAlignment="Center" Background="#CCCCCC" Margin="0,0,1,1" FontWeight="Bold">X-Axis</Label>
                        <Label VerticalContentAlignment="Center" Grid.Row="0" Grid.Column="2" FontSize="12" HorizontalContentAlignment="Center" Background="#CCCCCC" Margin="0,0,1,1" FontWeight="Bold">Y-Axis</Label>
                        <Label VerticalContentAlignment="Center" Grid.Row="0" Grid.Column="3" FontSize="12" HorizontalContentAlignment="Center" Background="#CCCCCC" Margin="0,0,1,1" FontWeight="Bold">Z-Axis</Label>
                        <Label VerticalContentAlignment="Center" Grid.Row="0" Grid.Column="4" FontSize="12" HorizontalContentAlignment="Center" Background="#CCCCCC" Margin="0,0,1,1" FontWeight="Bold">Spare</Label>
                        <Label VerticalContentAlignment="Center" Grid.Row="1" Grid.Column="0" FontSize="12" HorizontalContentAlignment="Center" Background="#CCCCCC" Margin="0,0,1,1" FontWeight="Bold">Target Position</Label>
                        <Label VerticalContentAlignment="Center" Grid.Row="2" Grid.Column="0" FontSize="12" HorizontalContentAlignment="Center" Background="#CCCCCC" Margin="0,0,1,1" FontWeight="Bold">Current Position</Label>
                        <Label VerticalContentAlignment="Center" Grid.Row="3" Grid.Column="0" FontSize="12" HorizontalContentAlignment="Center" Background="#CCCCCC" Margin="0,0,1,1" FontWeight="Bold">Diference Value</Label>
                        <Image x:Name="imgPattern" Grid.Column="0" Source="/Resource/Image/1.PNG"></Image>
                        <Grid Grid.Column="1" Grid.Row="1" Margin="2,2,2,2" Background="#222222">
                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                <TextBox Name="txtPositionDataX" Background="Black" Foreground="Yellow" FontSize="18" FontFamily="Agency FB" HorizontalContentAlignment="Center" VerticalContentAlignment="Center">350.00</TextBox>
                                <Label Foreground="#BBBBBB" VerticalContentAlignment="Center" HorizontalAlignment="Right" FontSize="12">mm</Label>
                            </StackPanel>
                        </Grid>
                        <Grid Grid.Column="2" Grid.Row="1" Margin="2,2,2,2" Background="#222222">
                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                <TextBox Name="txtPositionDataY" Background="Black" Foreground="Yellow" FontSize="18" FontFamily="Agency FB" HorizontalContentAlignment="Center" VerticalContentAlignment="Center">350.00</TextBox>
                                <Label Foreground="#BBBBBB" VerticalContentAlignment="Center" HorizontalAlignment="Right" FontSize="12">mm</Label>
                            </StackPanel>
                        </Grid>
                        <Grid Grid.Column="3" Grid.Row="1" Margin="2,2,2,2" Background="#222222">
                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                <PLC:DataDWord_D
                                    Device="0"
                                    Foreground="Yellow"
                                    NoOfDisplay="3"
                                    NoOfDecimalDigits="3"
                                    Background="Black"
                                    IsReadOnly="True"
                                    FontSize="18"
                                    FontFamily="Agency FB"
                                ></PLC:DataDWord_D>
                                <Label Foreground="#BBBBBB" VerticalContentAlignment="Center" HorizontalAlignment="Right" FontSize="12">mm</Label>
                            </StackPanel>
                        </Grid>
                        <Grid Grid.Column="4" Grid.Row="1" Margin="2,2,2,2" Background="#222222">
                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                <!--<TextBox Name="txtPositionDataU" FontSize="18" FontFamily="Agency FB" >0.00</TextBox>
                                <Label Foreground="#BBBBBB" VerticalContentAlignment="Center" HorizontalAlignment="Right" FontSize="12">mm</Label>-->
                            </StackPanel>
                        </Grid>
                        <Grid Grid.Column="1" Grid.Row="2" Margin="2,2,2,2" Background="#222222">
                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                <PLC:DataDWord_D
                                    x:Name="currentX"
                                    Device="1500"
                                    Foreground="#00EEFF"
                                    NoOfDisplay="3"
                                    NoOfDecimalDigits="3"
                                    Background="Black"
                                    IsReadOnly="True"
                                    FontSize="18"
                                    FontFamily="Agency FB"
                                ></PLC:DataDWord_D>
                                <Label Foreground="#BBBBBB" VerticalContentAlignment="Center" HorizontalAlignment="Right" FontSize="12">mm</Label>
                            </StackPanel>
                        </Grid>
                        <Grid Grid.Column="2" Grid.Row="2" Margin="2,2,2,2" Background="#222222">
                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                <PLC:DataDWord_D
                                    Device="1502"
                                    x:Name="currentY"
                                    Foreground="#00EEFF"
                                    NoOfDisplay="3"
                                    NoOfDecimalDigits="3"
                                    Background="Black"
                                    IsReadOnly="True"
                                    FontSize="18"
                                    FontFamily="Agency FB"
                                ></PLC:DataDWord_D>
                                <Label Foreground="#BBBBBB" VerticalContentAlignment="Center" HorizontalAlignment="Right" FontSize="12">mm</Label>
                            </StackPanel>
                        </Grid>
                        <Grid Grid.Column="3" Grid.Row="2" Margin="2,2,2,2" Background="#222222">
                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                <PLC:DataDWord_D
                                    Device="1504"
                                    Foreground="#00EEFF"
                                    NoOfDisplay="3"
                                    NoOfDecimalDigits="3"
                                    Background="Black"
                                    IsReadOnly="True"
                                    FontSize="18"
                                    FontFamily="Agency FB"
                                ></PLC:DataDWord_D>
                                <Label Foreground="#BBBBBB" VerticalContentAlignment="Center" HorizontalAlignment="Right" FontSize="12">mm</Label>
                            </StackPanel>
                        </Grid>
                        <Grid Grid.Column="4" Grid.Row="2" Margin="2,2,2,2" Background="#222222">
                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                <!--<TextBox Name="ActualPositionU" FontSize="18" FontFamily="Agency FB">0.00</TextBox>
                                <Label Foreground="#BBBBBB" VerticalContentAlignment="Center" HorizontalAlignment="Right" FontSize="12">mm</Label>-->
                            </StackPanel>
                        </Grid>
                        <Grid Grid.Column="1" Grid.Row="3" Margin="2,2,2,2" Background="#222222">
                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                <PLC:DataDWord_D
                                    Device="0"
                                    Foreground="White"
                                    NoOfDisplay="3"
                                    NoOfDecimalDigits="3"
                                    Background="Black"
                                    IsReadOnly="True"
                                    FontSize="18"
                                    FontFamily="Agency FB"
                                ></PLC:DataDWord_D>
                                <Label Foreground="#BBBBBB" VerticalContentAlignment="Center" HorizontalAlignment="Right" FontSize="12">mm</Label>
                            </StackPanel>
                        </Grid>
                        <Grid Grid.Column="2" Grid.Row="3" Margin="2,2,2,2" Background="#222222">
                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                <PLC:DataDWord_D
                                    Device="0"
                                    Foreground="White"
                                    NoOfDisplay="3"
                                    NoOfDecimalDigits="3"
                                    Background="Black"
                                    IsReadOnly="True"
                                    FontSize="18"
                                    FontFamily="Agency FB"
                                ></PLC:DataDWord_D>
                                <Label Foreground="#BBBBBB" VerticalContentAlignment="Center" HorizontalAlignment="Right" FontSize="12">mm</Label>
                            </StackPanel>
                        </Grid>
                        <Grid Grid.Column="3" Grid.Row="3" Margin="2,2,2,2" Background="#222222">
                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                <PLC:DataDWord_D
                                    Device="0"
                                    Foreground="White"
                                    NoOfDisplay="3"
                                    NoOfDecimalDigits="3"
                                    Background="Black"
                                    IsReadOnly="True"
                                    FontSize="18"
                                    FontFamily="Agency FB"
                                ></PLC:DataDWord_D>
                                <Label Foreground="#BBBBBB" VerticalContentAlignment="Center" HorizontalAlignment="Right" FontSize="12">mm</Label>
                            </StackPanel>
                        </Grid>
                        <Grid Grid.Column="4" Grid.Row="3" Margin="2,2,2,2" Background="#222222">
                            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                                <!--<TextBox Name="DeferencePositionU" FontSize="18" FontFamily="Agency FB">0.00</TextBox>
                                <Label Foreground="#BBBBBB" VerticalContentAlignment="Center" HorizontalAlignment="Right" FontSize="12">mm</Label>-->
                            </StackPanel>
                        </Grid>
                    </Grid>

                </Grid>
                <Grid Grid.Column="1" Margin="10,0,0,0">
                </Grid>
                <Grid Grid.Column="2">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="1.2*"></RowDefinition>
                        <RowDefinition Height="5*"></RowDefinition>
                    </Grid.RowDefinitions>
                    <Grid Grid.Row="1">
                        <Grid Grid.Column="0">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="1*"></RowDefinition>
                                <RowDefinition Height="2*"></RowDefinition>
                            </Grid.RowDefinitions>
                            <Label Grid.Row="0" Margin="5,5,5,5" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontWeight="Bold" Background="Aqua">STATUS ROBOT</Label>
                            <Grid Grid.Row="1">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="1*"></RowDefinition>
                                    <RowDefinition Height="1*"></RowDefinition>
                                </Grid.RowDefinitions>
                                <Grid Grid.Row="0">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="1*"></ColumnDefinition>
                                        <ColumnDefinition Width="1*"></ColumnDefinition>
                                        <ColumnDefinition Width="1*"></ColumnDefinition>
                                    </Grid.ColumnDefinitions>
                                    <Label Grid.Row="0" Name="lblStRun" Grid.Column="0" Margin="5,5,5,5" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="Gold" FontWeight="Bold">R</Label>
                                    <Label Grid.Row="0" Name="lblStStop" Grid.Column="1" Margin="5,5,5,5" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="Gold" FontWeight="Bold">S</Label>
                                    <Label Grid.Row="0" Name="lblStError" Grid.Column="2" Margin="5,5,5,5" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="Gold" FontWeight="Bold">E</Label>
                                </Grid>
                                <Button x:Name="btnSelectPatern" Grid.Row="1" Foreground="Black" Background="LightGray" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontWeight="Bold" FontStyle="Italic" FontSize="20" Margin="2,2,2,2" Content="Select Patern"></Button>
                            </Grid>
                        </Grid>
                    </Grid>
                </Grid>
            </Grid>
            <Border Grid.Row="2" Margin="2,2,2,2" BorderBrush="Silver" Background="#FFFFFF" BorderThickness="1" CornerRadius="2,2,2,2">
                <Grid Grid.Row="2">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="2*"></ColumnDefinition>
                        <ColumnDefinition></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <Grid Grid.Column="0">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="1*"></RowDefinition>
                            <RowDefinition Height="5.5*"></RowDefinition>
                        </Grid.RowDefinitions>
                        <Border Grid.Row="0" Margin="2,2,2,2" BorderThickness="1" BorderBrush="#222222">
                            <Label Margin="2,2,2,2" BorderThickness="0" Background="#AAAAAA" FontWeight="Bold" Foreground="White" HorizontalContentAlignment="Center">Select the location you want to move(이동하려는 위치를 선택하십요)</Label>
                        </Border>
                        <Grid x:Name="gridPos" Grid.Row="1">
                            <Grid.Resources>
                                <Style TargetType="{x:Type Label}">
                                    <Setter Property="BorderThickness" Value="0" />
                                    <Setter Property="Margin" Value="1,1,1,1" />
                                    <Setter Property="Padding" Value="2,0,2,0" />
                                    <Setter Property="VerticalContentAlignment" Value="Center" />
                                    <Setter Property="HorizontalContentAlignment" Value="Center" />
                                    <Setter Property="FontSize" Value="8" />
                                    <Setter Property="FontWeight" Value="Normal" />
                                </Style>
                                <Style TargetType="TextBlock">
                                    <Setter Property="HorizontalAlignment" Value="Center" />
                                </Style>
                            </Grid.Resources>
                        </Grid>
                    </Grid>
                    <Grid Grid.Column="1">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="1*"></RowDefinition>
                            <RowDefinition Height="5.5*"></RowDefinition>
                        </Grid.RowDefinitions>
                        <Border Grid.Row="0" Margin="5,2,5,2" BorderThickness="1" BorderBrush="#222222">
                            <Label Margin="2,2,2,2" BorderThickness="0" Background="#AAAAAA" FontWeight="Bold" Foreground="White" HorizontalContentAlignment="Center">Edit Position</Label>
                        </Border>
                        <Grid Grid.Row="1" Margin="5,2,5,2" Background="DarkKhaki">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition></ColumnDefinition>
                                <ColumnDefinition></ColumnDefinition>
                                <ColumnDefinition></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="0.5*"></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                            </Grid.RowDefinitions>
                            <Label Name="lblId" Grid.Column="0" Grid.Row="0" Foreground="White" Background="DarkCyan" Margin="1,1,1,1" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="15" FontWeight="Bold">1</Label>
                            <Label Grid.Column="1" Grid.Row="0" Foreground="White" Background="DarkCyan" Margin="1,1,1,1" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="15" FontWeight="Bold">X</Label>
                            <Label Grid.Column="2" Grid.Row="0" Foreground="White" Background="DarkCyan" Margin="1,1,1,1" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="15" FontWeight="Bold">Y</Label>
                            <Label Grid.Column="0" Grid.Row="1" Background="LightSalmon" Margin="1,1,1,1" Content="Position&#10;Target" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" FontSize="13" FontWeight="Bold"></Label>
                            <Grid Margin="1,1,1,1" Grid.Row="2" Grid.Column="0" Background="LightSalmon">
                                <CheckBox x:Name="cbUseEncoder" FontWeight="Bold" Grid.Row="1" Grid.Column="0" Content="Use Encoder" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" Background="MintCream"></CheckBox>
                            </Grid>
                            <Grid Grid.Column="1" Grid.Row="1" Margin="2,15,2,15" Background="LightGray">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition></ColumnDefinition>
                                    <ColumnDefinition Width="0.5*"></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Label Grid.Column="0" x:Name="lblPosX"  HorizontalContentAlignment="Right" VerticalContentAlignment="Center" FontSize="13" FontWeight="Bold">0.00</Label>
                                <Label Grid.Column="1" Grid.Row="1" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="10">mm</Label>
                            </Grid>
                            <Grid Grid.Column="2" Grid.Row="1" Margin="2,15,2,15" Background="LightGray">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition></ColumnDefinition>
                                    <ColumnDefinition Width="0.5*"></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Label Grid.Column="0" x:Name="lblPosY"  HorizontalContentAlignment="Right" VerticalContentAlignment="Center" FontSize="13" FontWeight="Bold">0.00</Label>
                                <Label Grid.Column="1" Grid.Row="1" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="10">mm</Label>
                            </Grid>
                            <Grid Grid.Column="1" Grid.Row="2" Margin="2,15,2,15" >
                                <Grid.RowDefinitions>
                                    <RowDefinition></RowDefinition>
                                    <RowDefinition></RowDefinition>
                                </Grid.RowDefinitions>
                                <Grid Grid.Row="0">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition></ColumnDefinition>
                                        <ColumnDefinition Width="0.5*"></ColumnDefinition>
                                    </Grid.ColumnDefinitions>
                                    <TextBox x:Name="txtChangePosX" Grid.Column="0"  Background="MintCream" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="13" FontWeight="Bold">
                                        <TextBox.Style>
                                            <Style TargetType="TextBox">
                                                <Setter Property="Text" Value="{Binding ElementName=currentX, Path=Text}" />
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding ElementName=cbUseEncoder, Path=IsChecked}" Value="False">
                                                        <Setter Property="Text" Value="{x:Null}" />
                                                    </DataTrigger>
                                                    <DataTrigger Binding="{Binding ElementName=cbUseEncoder, Path=IsChecked}" Value="True">
                                                        <Setter Property="Text" Value="{Binding ElementName=currentX, Path=Text}" />
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </TextBox.Style>
                                    </TextBox>
                                    <Label Grid.Column="1" Grid.Row="1"   HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="10">mm</Label>
                                </Grid>
                                <Grid Grid.Row="1">
                                    <CheckBox x:Name="cbUseWithTargetX" FontWeight="Bold" Grid.Row="1" FontSize="12" Grid.Column="0" Content="Use with target" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" Background="MintCream"></CheckBox>
                                </Grid>
                            </Grid>
                            <Grid Grid.Column="2" Grid.Row="2" Margin="2,15,2,15">
                                <Grid.RowDefinitions>
                                    <RowDefinition></RowDefinition>
                                    <RowDefinition></RowDefinition>
                                </Grid.RowDefinitions>
                                <Grid Grid.Row="0">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition></ColumnDefinition>
                                        <ColumnDefinition Width="0.5*"></ColumnDefinition>
                                    </Grid.ColumnDefinitions>
                                    <TextBox x:Name="txtChangePosY"  HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="13" FontWeight="Bold">
                                        <TextBox.Style>
                                            <Style TargetType="TextBox">
                                                <Setter Property="Text" Value="{Binding ElementName=currentY, Path=Text}" />
                                                <Style.Triggers>
                                                    <DataTrigger Binding="{Binding ElementName=cbUseEncoder, Path=IsChecked}" Value="False">
                                                        <Setter Property="Text" Value="{x:Null}" />
                                                    </DataTrigger>
                                                </Style.Triggers>
                                            </Style>
                                        </TextBox.Style>
                                    </TextBox>
                                    <Label Grid.Column="1"  Grid.Row="1"   HorizontalContentAlignment="Center" VerticalContentAlignment="Center" FontSize="10">mm</Label>
                                </Grid>
                                <Grid Grid.Row="1">
                                    <CheckBox x:Name="cbUseWithTargetY" FontWeight="Bold" Grid.Row="1" FontSize="12" Grid.Column="0" Content="Use with target" HorizontalContentAlignment="Left" VerticalContentAlignment="Center" Background="MintCream"></CheckBox>
                                </Grid>
                            </Grid>
                            <Button x:Name="btnChangePos" FontWeight="Bold" FontFamily="Time new roman" Grid.Column="1" Grid.Row="3" Margin="1,5,1,5">Change</Button>
                            <Button x:Name="btnSavePos" FontWeight="Bold"  FontFamily="Time new roman" Grid.Column="2" Grid.Row="3" Margin="1,5,1,5">Save</Button>
                        </Grid>
                    </Grid>

                </Grid>
            </Border>
            <TabControl Name="TabControl" Grid.Row="3">
                <TabItem FontSize="15" Height="40" Header="Motor Control">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="0.6*"></ColumnDefinition>
                            <ColumnDefinition Width="0.7*"></ColumnDefinition>
                            <ColumnDefinition Width="2*"></ColumnDefinition>
                            <ColumnDefinition Width="0.7*"></ColumnDefinition>
                            <ColumnDefinition Width="0.7*"></ColumnDefinition>
                            <ColumnDefinition Width="0.5*"></ColumnDefinition>
                        </Grid.ColumnDefinitions>
                        <Border Margin="5,5,5,5" BorderBrush="Silver" Background="#FFFFFF" BorderThickness="1" CornerRadius="8,8,8,8" Grid.Column="0">
                            <Grid Grid.Column="2" Margin="5,0,5,0">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="30"></RowDefinition>
                                    <RowDefinition Height="3*"></RowDefinition>
                                </Grid.RowDefinitions>
                                <Label VerticalContentAlignment="Center" FontWeight="Bold" FontSize="14" HorizontalContentAlignment="Center">[Move Robot]</Label>
                                <Grid Grid.Row="1">
                                    <Image Source="/Resource/Image/Move.png"></Image>
                                    <PLC:PLCButton
                                        Grid.Column="0"
                                        Margin="2"
                                        Action="Set"
                                        DeviceWrite="M" AddressWrite="1810"
                                        UseLampCodition="False"
                                        Background="Transparent"
                                        BackgroundLampOFF="Transparent"
                                        BackgroundLampON="Transparent"
                                        TextOFF="Move"
                                        TextON="Move"
                                        Content="Move"
                                        Foreground="Black"
                                        BorderThickness="1"
                                        BorderBrush="Black"
                                        FontSize="12">
                                    </PLC:PLCButton>
                                </Grid>
                            </Grid>
                        </Border>
                        <Border Margin="5,5,5,5" BorderBrush="Silver" Background="#FFFFFF" BorderThickness="1" CornerRadius="8,8,8,8" Grid.Column="1">
                            <Grid Grid.Column="0" Margin="5,0,5,0">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="30"></RowDefinition>
                                    <RowDefinition Height="1*"></RowDefinition>
                                    <RowDefinition Height="1*"></RowDefinition>
                                    <RowDefinition Height="1*"></RowDefinition>
                                    <RowDefinition Height="1*"></RowDefinition>
                                </Grid.RowDefinitions>
                                <Label VerticalContentAlignment="Center" FontWeight="Bold" FontSize="14" HorizontalContentAlignment="Center">[Robot Home]</Label>
                                <PLC:PLCButton
                                    Grid.Row="1"
                                    Margin="0,0,0,5"
                                    Action="Set"
                                    DeviceWrite="M" AddressWrite="1795"
                                    UseLampCodition="True"
                                    DeviceLamp="M" AddressLamp="1795"
                                    Background="LightGray"
                                    BackgroundLampOFF="LightGray"
                                    BackgroundLampON="OrangeRed"
                                    TextOFF="ALL HOME"
                                    TextON="ALL HOME"
                                    Content="ALL HOME"
                                    Foreground="Black"
                                    BorderBrush="Black"
                                    BorderThickness="1">
                                </PLC:PLCButton>
                                <PLC:PLCButton
                                    Grid.Row="2"
                                    Margin="0,0,0,5"
                                    Action="Set"
                                    DeviceWrite="M" AddressWrite="1792"
                                    UseLampCodition="True"
                                    DeviceLamp="M" AddressLamp="1792"
                                    Background="LightGray"
                                    BackgroundLampOFF="LightGray"
                                    BackgroundLampON="OrangeRed"
                                    TextOFF="X-HOME"
                                    TextON="X-HOME"
                                    Content="X-HOME"
                                    Foreground="Black"
                                    BorderBrush="Black"
                                    BorderThickness="1">
                                </PLC:PLCButton>
                                <PLC:PLCButton
                                    Grid.Row="3"
                                    Margin="0,0,0,5"
                                    Action="Set"
                                    DeviceWrite="M" AddressWrite="1793"
