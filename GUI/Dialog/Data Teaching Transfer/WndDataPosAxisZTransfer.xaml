<Window x:Class="GUI.WndDataPosAxisZTransfer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI"
        xmlns:device="clr-namespace:DevicePLC;assembly=DevicePLC"
        mc:Ignorable="d"
        Title="WndDataPosAxisZTransfer" WindowStartupLocation="CenterScreen" Height="450" Width="800">
    <Grid>
        <Grid.Background>
            <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                <GradientStop Color="Black"/>
                <GradientStop Color="#FFB3FBFB" Offset="1"/>
            </LinearGradientBrush>
        </Grid.Background>
        <Grid.Resources>
            <Style x:Key="lableName" TargetType="Label">
                <Setter Property="FontSize" Value="25"></Setter>
                <Setter Property="Background" Value="White"></Setter>
                <Setter Property="FontFamily" Value="Bahnschrift"></Setter>
                <Setter Property="HorizontalContentAlignment" Value="Left"></Setter>
                <Setter Property="VerticalContentAlignment" Value="Center"></Setter>
                <Setter Property="Foreground" Value="Black"></Setter>
                <Setter Property="Margin" Value="2"></Setter>
            </Style>
            <Style x:Key="lableNo" TargetType="Label">
                <Setter Property="FontSize" Value="25"></Setter>
                <Setter Property="Background" Value="White"></Setter>
                <Setter Property="FontFamily" Value="Bahnschrift"></Setter>
                <Setter Property="HorizontalContentAlignment" Value="Center"></Setter>
                <Setter Property="VerticalContentAlignment" Value="Center"></Setter>
                <Setter Property="Foreground" Value="Black"></Setter>
                <Setter Property="Margin" Value="2"></Setter>
            </Style>
            <Style x:Key="lableType" TargetType="Label">
                <Setter Property="FontSize" Value="25"></Setter>
                <Setter Property="Background" Value="White"></Setter>
                <Setter Property="FontFamily" Value="Bahnschrift"></Setter>
                <Setter Property="HorizontalContentAlignment" Value="Center"></Setter>
                <Setter Property="VerticalContentAlignment" Value="Center"></Setter>
                <Setter Property="Foreground" Value="Black"></Setter>
                <Setter Property="Margin" Value="2"></Setter>
            </Style>
            <Style x:Key="lableUnit" TargetType="Label">
                <Setter Property="FontSize" Value="25"></Setter>
                <Setter Property="Background" Value="White"></Setter>
                <Setter Property="FontFamily" Value="Bahnschrift"></Setter>
                <Setter Property="HorizontalContentAlignment" Value="Center"></Setter>
                <Setter Property="VerticalContentAlignment" Value="Center"></Setter>
                <Setter Property="Foreground" Value="Black"></Setter>
                <Setter Property="Margin" Value="2"></Setter>
            </Style>
        </Grid.Resources>
        <Grid Grid.Row="1">
            <Grid.RowDefinitions>
                <RowDefinition Height="50"></RowDefinition>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="60"></ColumnDefinition>
                <ColumnDefinition Width="2*"></ColumnDefinition>
                <ColumnDefinition Width="1*"></ColumnDefinition>
                <ColumnDefinition Width="1*"></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <Label Margin="2" Grid.Row="0" Grid.Column="0" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="#FF9FDABA" FontSize="25" FontWeight="Bold">NO.</Label>
            <Label Margin="2" Grid.Row="0" Grid.Column="1" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="#FF9FDABA" FontSize="25" FontWeight="Bold">DESCRIPTION</Label>
            <Label Margin="2" Grid.Row="0" Grid.Column="2" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="#FF9FDABA" FontSize="25" FontWeight="Bold">Speed(mm/s)</Label>
            <Label Margin="2" Grid.Row="0" Grid.Column="3" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Background="#FF9FDABA" FontSize="25" FontWeight="Bold">Data(mm)</Label>

            <Label Grid.Row="1" Grid.Column="0" Style="{StaticResource lableNo}">1</Label>
            <Label Grid.Row="2" Grid.Column="0" Style="{StaticResource lableNo}">2</Label>
            <Label Grid.Row="3" Grid.Column="0" Style="{StaticResource lableNo}">3</Label>
            <Label Grid.Row="4" Grid.Column="0" Style="{StaticResource lableNo}">4</Label>
            <Label Grid.Row="5" Grid.Column="0" Style="{StaticResource lableNo}">5</Label>
            <Label Grid.Row="6" Grid.Column="0" Style="{StaticResource lableNo}">6</Label>
            <Label Grid.Row="7" Grid.Column="0" Style="{StaticResource lableNo}">7</Label>
            <Label Grid.Row="8" Grid.Column="0" Style="{StaticResource lableNo}">8</Label>
            <Label Grid.Row="9" Grid.Column="0" Style="{StaticResource lableNo}">9</Label>
            <Label Grid.Row="10" Grid.Column="0" Style="{StaticResource lableNo}">10</Label>

            <Label Grid.Row="1" Grid.Column="1" Style="{StaticResource lableName}">UP</Label>
            <Label Grid.Row="2" Grid.Column="1" Style="{StaticResource lableName}">DN Matrix Tray (Sp High)</Label>
            <Label Grid.Row="3" Grid.Column="1" Style="{StaticResource lableName}">DN Matrix Tray (Sp Low)</Label>
            <Label Grid.Row="4" Grid.Column="1" Style="{StaticResource lableName}">UP Matrix Tray (Sp Low)</Label>
            <Label Grid.Row="5" Grid.Column="1" Style="{StaticResource lableName}">DN Centering (Sp High)</Label>
            <Label Grid.Row="6" Grid.Column="1" Style="{StaticResource lableName}">DN Centering (Sp Low)</Label>
            <Label Grid.Row="7" Grid.Column="1" Style="{StaticResource lableName}">UP Centering (Sp Low)</Label>
            <Label Grid.Row="8" Grid.Column="1" Style="{StaticResource lableName}">DN Buffer 1</Label>
            <Label Grid.Row="9" Grid.Column="1" Style="{StaticResource lableName}">DN Buffer 2</Label>
            <Label Grid.Row="10" Grid.Column="1" Style="{StaticResource lableName}"></Label>

            <Grid Grid.Row="1" Grid.Column="3" Margin="2">
