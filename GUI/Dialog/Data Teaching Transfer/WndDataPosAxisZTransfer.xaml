            </Grid>
            <Grid Grid.Row="2" Grid.Column="3" Margin="2">
                <device:DataDWord_D FontFamily="Agency FB" NoOfDecimalDigits="3" IsShowInWindow="True" NoOfDisplay="4" Device="5042" IsReadOnly="False" Background="Black" Foreground="Yellow" Grid.ColumnSpan="2"></device:DataDWord_D>
